switch.purple[checked] .wx-switch-input,checkbox.purple[checked] .wx-checkbox-input,radio.purple[checked] .wx-radio-input,switch.purple.checked .uni-switch-input,checkbox.purple.checked .uni-checkbox-input,radio.purple.checked .uni-radio-input {background-color: #6739b6 !important;border-color: #6739b6 !important;color: #ffffff !important;}
