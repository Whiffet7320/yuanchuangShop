.cu-dialog {
	width: 80%;
	overflow: initial;
	text-align: left;
	border-radius: 25rpx;
}
.cu-dialog .hade {
	position: relative;
	width: 100%;
	top: -128rpx;
}
.cu-dialog .hade .title {
	position: relative;
	top: -280rpx;
	text-align: right;
	padding-right: 50rpx;
	overflow: hidden;
	height: 110rpx;
	margin-bottom: -110rpx;
}
.cu-dialog .hade .title>view {
	font-size: 50rpx;
}
.cu-dialog .info {
	position: relative;
	display: block;
	top: -170rpx;
	margin: 0 30rpx -140rpx 30rpx;
	line-height: 1.8;
}
.cu-bar .search-form {
	background-color: rgba(242, 242, 242, 0.9);
}
.cu-bar .action:last-child {
	margin-right: 0;
}



