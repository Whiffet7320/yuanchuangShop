
.wanl-footprint .box {
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
}
.wanl-footprint .box:after { 
	content: ""; 
	width: 33%;
}
.wanl-footprint .box .item {
	width: 33%;
	background-color: #ffffff;
	margin-bottom: 5rpx;
}
.wanl-footprint .box .item image{
	height: 230rpx;
}
.wanl-footprint .empty{
	background-color: #f2f2f2;
	line-height: 230rpx;
	text-align: center;
	height: 230rpx;
	width: 100%;
	font-size: 100rpx;
}
.wanl-footprint .empty text{
	color: #d5d5d5;
}

